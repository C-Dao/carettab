{
  "name": "carettab",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng build --watch",
    "build": "ng build --prod",
    "build:ext": "web-ext build --source-dir=dist --artifacts-dir=.",
    "lint": "ng lint",
    "lint:webext": "web-ext lint --source-dir ./dist",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en-US.json ./src/assets/i18n/de-DE.json ./src/assets/i18n/es-MX.json ./src/assets/i18n/fr-FR.json ./src/assets/i18n/ja-JP.json ./src/assets/i18n/ko-KR.json  ./src/assets/i18n/ru-RU.json ./src/assets/i18n/uk-UA.json ./src/assets/i18n/zh-CN.json --sort --format namespaced-json -m _"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.4",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "7.1.4",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/router": "7.1.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/chrome": "0.0.65",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.6",
    "core-js": "^2.4.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "8.3.21",
    "@angular/compiler-cli": "7.1.4",
    "@angular/language-service": "7.1.4",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/node": "~6.0.60",
    "codelyzer": "^5.2.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "3.1.6"
  }
}
